{
  "name": "discord-chunker",
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build:web": "tsx web/build.ts",
    "build": "pnpm build:web",
    "dev": "pnpm build:web && wrangler dev",
    "deploy": "pnpm build:web && wrangler deploy",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "format": "biome format --write",
    "pretest": "pnpm build:web",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@cloudflare/vitest-pool-workers": "^0.12.10",
    "@cloudflare/workers-types": "^4.20260210.0",
    "esbuild": "^0.27.3",
    "lefthook": "^2.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "~3.2.4",
    "wrangler": "^4.63.0"
  },
  "dependencies": {
    "discord-markdown": "2.5.1"
  }
}
